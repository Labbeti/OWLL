        <owl:inverseOf rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#publish"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#TabletopGame"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Publisher"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#play -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#play">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Player"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#TabletopGame"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#publish -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#publish">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Publisher"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#TabletopGame"/>
    </owl:ObjectProperty>
    


