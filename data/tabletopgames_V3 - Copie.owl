    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#DiceSixSided -->

    <owl:Class rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#DiceSixSided">
