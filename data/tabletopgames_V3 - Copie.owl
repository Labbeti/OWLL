    </owl:Class>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Card -->

    <owl:Class rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Card">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Object"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#CardGame -->

    <owl:Class rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#CardGame">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#TabletopGame"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#contains"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Card"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#contains"/>
                        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:maxQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#GameBoard"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Chess -->

    <owl:Class rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Chess">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#StrategyGame"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#isPlayedBy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Player"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="fr">Echecs</rdfs:label>
    </owl:Class>
    


