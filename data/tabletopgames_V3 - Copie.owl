        <owl:inverseOf rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#isInside"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Object"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#isInside -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#isInside">
