        <rdfs:range rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Player"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#isPublishedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#isPublishedBy">
