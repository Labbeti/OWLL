    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#AdventureGame -->

    <owl:Class rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#AdventureGame">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#TabletopGame"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Battleship -->

    <owl:Class rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Battleship">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#PaperGame"/>
        <rdfs:label xml:lang="fr">Bataille Navale</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#BoardGame -->

    <owl:Class rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#BoardGame">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#TabletopGame"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#contains"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#GameBoard"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#PaperGame"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Book -->

    <owl:Class rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Book">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Object"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Carcassonne -->

    <owl:Class rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Carcassonne">
