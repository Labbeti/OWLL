    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#chance -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#chance">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Game"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#duration -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#duration">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#TabletopGame"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#name -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#name">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Publisher"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#nbSides -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#nbSides">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Dice"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#releaseDate -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#releaseDate">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Game"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#strategy -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#strategy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/invite/ontologies/2019/4/tabletopgames#Game"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
    </owl:DatatypeProperty>
    


